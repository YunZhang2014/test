{
  "name": "investment-tracker",
  "description": "Jasmine Testing Example Application",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node bin/server.js",
    "test": "./node_modules/.bin/jshint . && ./node_modules/.bin/jsxhint . && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "./node_modules/.bin/webpack -p",
    "dev": "./node_modules/.bin/webpack-dev-server --inline --hot"
  },
  "dependencies": {
    "express": "4.x",
    "jquery": "^2.1.3",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "babel-core": "^4.6.1",
    "babel-loader": "^4.0.0",
    "css-loader": "^0.9.1",
    "es5-shim": "^4.1.0",
    "html-webpack-plugin": "^1.1.0",
    "jasmine-core": "^2.2.0",
    "jshint": "^2.6.3",
    "jsxhint": "^0.12.1",
    "karma": "^0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.5.0",
    "style-loader": "^0.8.3",
    "webpack": "^1.6.0",
    "webpack-dev-server": "^1.7.0"
  }
}
